[Desktop Entry]
Name=Malcolm Debug Logs
Exec=xfce4-terminal --command=/bin/bash\ -l\ -c\ "/usr/bin/python3\ ~/Malcolm/scripts/control.py\ --logs"
Comment=Monitor the debug output of Malcolm containers
Terminal=false
Type=Application
Icon=terminal.png
Categories=Network;
StartupNotify=true
X-XFCE-Source=file:///usr/share/applications/malcolm-logs.desktop